<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Network">
        <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="27.0" prefHeight="29.0" prefWidth="152.0" text="Choose Circle" textAlignment="CENTER" />
            <ComboBox fx:id="circle" layoutX="27.0" layoutY="56.0" onAction="#setNetwork" prefWidth="150.0" />
            <ComboBox fx:id="network" layoutX="27.0" layoutY="141.0" prefWidth="150.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="112.0" prefHeight="29.0" prefWidth="152.0" text="Choose Network" textAlignment="CENTER" />
            <Label alignment="CENTER" layoutX="463.0" layoutY="35.0" prefHeight="27.0" prefWidth="112.0" text="To Date" />
            <Label alignment="CENTER" layoutX="307.0" layoutY="35.0" prefHeight="27.0" prefWidth="112.0" text="From Date" />
            <DatePicker fx:id="to" layoutX="460.0" layoutY="70.0" prefHeight="31.0" prefWidth="119.0" />
            <DatePicker fx:id="from" layoutX="302.0" layoutY="70.0" prefHeight="31.0" prefWidth="119.0" />
            <Button fx:id="report" layoutX="458.0" layoutY="143.0" mnemonicParsing="false" onAction="#getReport" prefHeight="34.0" prefWidth="122.0" text="Get Report" />
            <Button fx:id="summary" layoutX="302.0" layoutY="143.0" mnemonicParsing="false" onAction="#getSummary" prefHeight="34.0" prefWidth="121.0" text="Get Summary" />
        </children>

</AnchorPane>
