<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="180.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Branch_Graph">
        <children>
            <ComboBox fx:id="circle" layoutX="9.0" layoutY="44.0" onAction="#setNetwork" prefWidth="150.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="8.0" layoutY="15.0" prefHeight="29.0" prefWidth="152.0" text="Choose Circle" textAlignment="CENTER" />
            <ComboBox fx:id="network" layoutX="184.0" layoutY="43.0" onAction="#setModule" prefWidth="150.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="183.0" layoutY="14.0" prefHeight="29.0" prefWidth="152.0" text="Choose Network" textAlignment="CENTER" />
            <ComboBox fx:id="module" layoutX="360.0" layoutY="43.0" onAction="#setRO" prefWidth="150.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="359.0" layoutY="14.0" prefHeight="29.0" prefWidth="152.0" text="Choose Module" textAlignment="CENTER" />
            <ComboBox fx:id="branch" layoutX="708.0" layoutY="43.0" prefWidth="150.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="707.0" layoutY="14.0" prefHeight="29.0" prefWidth="152.0" text="Choose Branch" textAlignment="CENTER" />
            <ComboBox fx:id="ro" layoutX="533.0" layoutY="43.0" onAction="#setBranch" prefWidth="150.0" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="532.0" layoutY="14.0" prefHeight="29.0" prefWidth="152.0" text="Choose RO" textAlignment="CENTER" />
            <Label alignment="CENTER" layoutX="647.0" layoutY="87.0" prefHeight="27.0" prefWidth="112.0" text="To Date" />
            <Label alignment="CENTER" layoutX="459.0" layoutY="87.0" prefHeight="27.0" prefWidth="112.0" text="From Date" />
            <DatePicker fx:id="to" layoutX="644.0" layoutY="122.0" prefHeight="31.0" prefWidth="119.0" />
            <DatePicker fx:id="from" layoutX="454.0" layoutY="122.0" prefHeight="31.0" prefWidth="119.0" />
            <Button fx:id="report" layoutX="644.0" layoutY="168.0" mnemonicParsing="false" onAction="#getReport" prefHeight="34.0" prefWidth="170.0" text="Get Detailed Report" />
            <Button fx:id="summary" layoutX="403.0" layoutY="168.0" mnemonicParsing="false" onAction="#getSummary" prefHeight="34.0" prefWidth="170.0" text="Get Summary Report" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="879.0" layoutY="14.0" prefHeight="29.0" prefWidth="152.0" text="Choose Graph Type" textAlignment="CENTER" />
      <ComboBox fx:id="type" layoutX="880.0" layoutY="43.0" prefWidth="150.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="1050.0" layoutY="14.0" prefHeight="29.0" prefWidth="152.0" text="Choose Period" textAlignment="CENTER" />
      <ComboBox fx:id="period" layoutX="1051.0" layoutY="43.0" prefWidth="150.0" />
        </children>
</AnchorPane>
