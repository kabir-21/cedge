        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="233.0" layoutY="24.0" prefHeight="29.0" prefWidth="152.0" text="Choose Bank" textAlignment="CENTER" />
        <Button fx:id="summary" layoutX="311.0" layoutY="190.0" mnemonicParsing="false" onAction="#getSummary" prefHeight="34.0" prefWidth="170.0" text="Get Summary Chart" />
        <Button fx:id="report" layoutX="520.0" layoutY="190.0" mnemonicParsing="false" onAction="#getReport" prefHeight="34.0" prefWidth="170.0" text="Get Detailed Chart" />
