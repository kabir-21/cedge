      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="614.0" layoutY="24.0" prefHeight="29.0" prefWidth="152.0" text="Choose Period" textAlignment="CENTER" />
      <ComboBox fx:id="period" layoutX="615.0" layoutY="53.0" prefWidth="150.0" />
    </children>

</AnchorPane>
