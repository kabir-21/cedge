<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="180.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Bank_Graph">
    <children>
        <ComboBox fx:id="bank" layoutX="169.0" layoutY="53.0" prefWidth="150.0" />
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="168.0" layoutY="24.0" prefHeight="29.0" prefWidth="152.0" text="Choose Bank" textAlignment="CENTER" />
        <Button fx:id="summary" layoutX="415.0" layoutY="191.0" mnemonicParsing="false" onAction="#getPlot" prefHeight="34.0" prefWidth="170.0" text="Get Plot" />
        <DatePicker fx:id="from" layoutX="362.0" layoutY="137.0" prefHeight="31.0" prefWidth="119.0" />
        <DatePicker fx:id="to" layoutX="520.0" layoutY="137.0" prefHeight="31.0" prefWidth="119.0" />
        <Label alignment="CENTER" layoutX="367.0" layoutY="102.0" prefHeight="27.0" prefWidth="112.0" text="From Date" />
