      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="510.0" layoutY="24.0" prefHeight="29.0" prefWidth="152.0" text="Choose Parameter" textAlignment="CENTER" />
      <ComboBox fx:id="type" layoutX="511.0" layoutY="53.0" onAction="#setPeriod" prefWidth="150.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="684.0" layoutY="24.0" prefHeight="29.0" prefWidth="152.0" text="Choose Period" textAlignment="CENTER" />
      <ComboBox fx:id="period" layoutX="685.0" layoutY="53.0" prefWidth="150.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="339.0" layoutY="24.0" prefHeight="29.0" prefWidth="152.0" text="Choose Chart Type" textAlignment="CENTER" />
      <ComboBox fx:id="chartType" layoutX="340.0" layoutY="53.0" onAction="#setParameters" prefWidth="150.0" />
    </children>

</AnchorPane>
